{
    "version": 2,
    "name": "caretech",
    "builds": [{ "src": "index.js", "use": "@vercel/node" }],
    "routes": [{ "src": "/(.*)", "dest": "index.js" }, { "handle": "filesystem" },
    {"src":"/caretech/v1/files", "dest": "./src/routes/files.routes.js"},
    {"src":"/caretech/v1/quotes", "dest": "./src/routes/quotes.routes.js"},
    {"src":"/caretech/v1/medicine", "dest": "./src/routes/medications.routes.js"},
    {"src":"/caretech/v1/category", "dest": "./src/routes/categories.routes.js"},
    {"src":"/caretech/v1/patient", "dest": "./src/routes/patients.routes.js"},
    {"src":"/caretech/v1/doctor", "dest": "./src/routes/doctors.routes.js"},
    {"src":"/caretech/v1/auth", "dest": "./src/routes/users.routes.js"},
    {"src":"/caretech/v1/service", "dest": "./src/routes/services.routes.js"},
    {"src":"/caretech/v1/prescription", "dest": "./src/routes/prescriptions.routes.js"},
    {"src":"/caretech/v1/bill", "dest": "./src/routes/bills.routes.js"}]
}