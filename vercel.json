{
    "version": 2,
    "name": "careTech",
    "builds": [
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "includeFiles": [
      "src/routes/**",
      "src/controllers/**",
      "src/models/**",
      "src/middlewares/**",
      "src/services/**"
    ]
  }